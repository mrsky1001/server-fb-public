{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "update": "git push; yarn update-core",
    "get-core": ". core-scripts/get_core.sh",
    "update-core": "(cd src/core/; . ../../core-scripts/update_core.sh) ",
    "init-core": "(cd src/core/; . ../../core-scripts/init_core.sh) ",
    "commit-core": "(cd src/core/; . ../../core-scripts/commit_core.sh) ",
    "pull-core": "(cd src/core/; . ../../core-scripts/pull_core.sh) ",
    "push-core": "(cd src/core/; . ../../core-scripts/push_core.sh) ",
    "remove-core": "(cd src/core/; . ../../core-scripts/remove_core.sh) ",
    "start": "forever -c \\\"yarn prod\\\" src",
    "build": "yarn --network-timeout 100000; yarn fix; tsc",
    "prod": "yarn fix; yarn kill-back; mkdir log; tsc && nodemon --exitcrash dist  >> log/foma_blog.log 2>> log/foma_blog_err.log",
    "dev": "yarn restart-db; yarn kill-back-dev; tsc && node dist/src/index.js",
    "dev-docker": "yarn && node ./dist/src/index.js",
    "kill-back-dev": "kill -9 `lsof -t -i tcp:8082`",
    "kill-back": "kill -9 `lsof -t -i tcp:8081`",
    "restart-db": "sudo service mongod restart",
    "fix": "eslint --fix src/**/*.{ts,js}; yaspeller-ci src/**/*.*",
    "yasp": "yaspeller-ci .",
    "test": "eslint --fix test; yarn kill-back; nyc ./node_modules/.bin/_mocha 'test/**/*-test.ts'"
  },
  "author": "mrsky1001 <nikita.nk16@yandex.ru>",
  "license": "ISC",
