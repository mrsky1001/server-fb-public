    "cyrillic-to-translit-js": "^3.2.1",
    "express": "^4.17.1",
    "express-validator": "^6.10.1",
    "forever": "^4.0.0",
    "fs": "^0.0.2",
    "image-size": "^1.0.0",
    "images": "^3.2.3",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "6.6.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.7.5",
    "npm": "^8.12.1",
    "process": "^0.11.10"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.1.2",
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.36",
    "@types/body-parser": "^1.19.1",
    "@types/chai": "^4.2.21",
